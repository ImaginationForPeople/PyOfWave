<!-- User information on annotations or records -->
<!DOCTYPE contrib [

<!ENTITY % wave SYSTEM "wave.dtd">
<!ATTLIST wave:a
	sel CDATA #IMPLIED	<!-- Wave Address of user who has selected the text -->
	cursor CDATA #IMPLIED>	<!-- Wave Address of user whose cursor is at the annotation -->

<!ENTITY % fetch SYSTEM "fetch.dtd">
<!ATTLIST fetch:record
	name PCDATA #IMPLIED
	status PCDATA #IMPLIED
	note PCDATA #IMPLIED
	avatar PCDATA #IMPLIED
	color PCDATA #IMPLIED
	lang PCDATA #IMPLIED>

]>